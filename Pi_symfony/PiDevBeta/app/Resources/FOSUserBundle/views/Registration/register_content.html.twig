<!-- Page -->
<div class="page" data-animsition-in="fade-in" data-animsition-out="fade-out">
    <div class="page-content">
        <div class="page-brand-info">
            <div class="brand">
                <img class="brand-img" src="{{ asset("template/topicon/assets/images/logo@2x.png")}}" alt="...">
                <h2 class="brand-text font-size-40">SocialPro</h2>
            </div>
            <p class="font-size-20">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
                tempor incididunt ut labore et dolore magna aliqua.</p>
        </div>
        <div class="page-register-main">
            <div class="brand hidden-md-up">
                <img class="brand-img" src="{{ asset("template/topicon/assets/images/logo-blue@2x.png")}}" alt="...">
                <h3 class="brand-text font-size-40">SocialPro</h3>
            </div>
            <h3 class="font-size-24">Sign Up</h3>
            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>



            {% trans_default_domain 'FOSUserBundle' %}

            {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'autocomplete':'off'}}) }}

            <div class="form-group form-material floating" data-plugin="formMaterial">

                {{ form_widget(form.email, {'attr':{ 'class': 'form-control empty' }}) }}
                {{ form_label(form.email, null, {'label_attr': {'class': 'floating-label'}}) }}
                {{ form_errors(form.email) }}

            </div>
            <div class="form-group form-material floating" data-plugin="formMaterial">

                {{ form_widget(form.username, {'attr':{ 'class': 'form-control empty' }}) }}
                {{ form_label(form.username, null, {'label_attr': {'class': 'floating-label'}}) }}
                {{ form_errors(form.username) }}

            </div>
            <div class="form-group form-material floating" data-plugin="formMaterial">

                {{ form_widget(form.plainPassword.first, {'attr':{ 'class': 'form-control empty' }}) }}
                {{ form_label(form.plainPassword.first, null, {'label_attr': {'class': 'floating-label'}}) }}
                {{ form_errors(form.plainPassword.first) }}

            </div>
            <div class="form-group form-material floating" data-plugin="formMaterial">

                {{ form_widget(form.plainPassword.second, {'attr':{ 'class': 'form-control empty' }}) }}
                {{ form_label(form.plainPassword.second, null, {'label_attr': {'class': 'floating-label'}}) }}
                {{ form_errors(form.plainPassword.second) }}

            </div>
            <div class="form-group clearfix">
                <div class="checkbox-custom checkbox-inline checkbox-primary float-left">
                    <input type="checkbox" id="inputCheckbox" name="term">
                    <label for="inputCheckbox"></label>
                </div>
                <p class="ml-35">By clicking Sign Up, you agree to our <a href="#">Terms</a>.</p>
            </div>
            <button type="submit" class="btn btn-primary btn-block">Sign Up</button>
            {{ form_end(form) }}


            <p>Have account already? Please go to <a href="{{ path('fos_user_security_login') }}">Sign In</a></p>
            <footer class="page-copyright">
                <p>WEBSITE BY amazingSurge</p>
                <p>Â© 2017. All RIGHT RESERVED.</p>
