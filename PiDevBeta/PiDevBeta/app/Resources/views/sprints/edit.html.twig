{% block body %}

    <!-- Page -->
    <div class="page">
        <div class="page-header">
            <h1 class="page-title">Edit Sprint</h1>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="../index.html">Home</a></li>
                <li class="breadcrumb-item"><a href="javascript:void(0)">Forms</a></li>
                <li class="breadcrumb-item active">Layouts</li>
            </ol>
            <div class="page-header-actions">
                <button type="button" class="btn btn-sm btn-icon btn-primary btn-round" data-toggle="tooltip"
                        data-original-title="Edit">
                    <i class="icon md-edit" aria-hidden="true"></i>
                </button>
                <button type="button" class="btn btn-sm btn-icon btn-primary btn-round" data-toggle="tooltip"
                        data-original-title="Refresh">
                    <i class="icon md-refresh-alt" aria-hidden="true"></i>
                </button>
                <button type="button" class="btn btn-sm btn-icon btn-primary btn-round" data-toggle="tooltip"
                        data-original-title="Setting">
                    <i class="icon md-settings" aria-hidden="true"></i>
                </button>
            </div>
        </div>

        <div class="page-content">
            <div class="panel">
                <div class="panel-body container-fluid">
                    <div class="row row-lg">
                        <div class="col-md-6">
                            <!-- Example Basic Form -->
                            <div class="example-wrap">
                                <h4 class="example-title">Edit form</h4>
                                <div class="example">

                                    {{ form_start(edit_form, {'method': 'post'}) }}

                                    <div class="row">

                                        <div class="col-md-6">
                                            <!-- Example Select Sizing -->
                                            <div class="example-wrap">
                                                <h4 class="example-title">Name Sprint</h4>

                                                <div class="example">

                                                    {{ form_widget(edit_form.name,{'attr': {'name':'name', 'type':'text','class': 'form-control', 'id':'nameSprint'}}) }}


                                                </div>

                                            </div>
                                            <!-- End Example Select Sizing -->
                                        </div>
                                    </div>

                                    <div class="col-md-6 col-xl-4">
                                        <!-- Example Inline -->
                                        <div class="example-wrap">
                                            <h4 class="example-title">Date Start Sprint</h4>

                                            <div class="example">

                                                {{ form_widget(edit_form.datestart,{'attr': {'name':'datestart', 'type':'hidden','class': 'form-control', 'id':'inputHiddenInline'}}) }}



                                                <input type="hidden" id="inputHiddenInline"  />
                                            </div>
                                        </div>
                                        <!-- End Example Inline -->
                                    </div>

                                    <div class="col-md-6 col-xl-4">
                                        <!-- Example Inline -->
                                        <div class="example-wrap">
                                            <h4 class="example-title">Deadline Sprint</h4>

                                            <div class="example">

                                                {{ form_widget(edit_form.deadline,{'attr': {'name':'deadline', 'type':'hidden','class': 'form-control', 'id':'inputHiddenInline'}}) }}



                                                <input type="hidden" id="inputHiddenInline"  />
                                            </div>
                                        </div>
                                        <!-- End Example Inline -->
                                    </div>

                                    <div class="row">

                                        <div class="col-md-6">
                                            <!-- Example Select Sizing -->
                                            <div class="example-wrap">
                                                <h4 class="example-title">Number Sprint</h4>

                                                <div class="example">

                                                    {{ form_widget(edit_form.number,{'attr': {'name':'number', 'type':'number','class': 'form-control', 'id':'numberSprint'}}) }}


                                                </div>

                                            </div>
                                            <!-- End Example Select Sizing -->
                                        </div>
                                    </div>


                                    <div class="form-group form-material">
                                        <label class="form-control-label" for="inputBasicEmail">Description Sprint</label>


                                        {{ form_widget(edit_form.description,{'attr': {'name':'description', 'type':'text','class': 'form-control', 'id':'descriptionSprint', 'placeholder':'Saisir la description'}}) }}
                                    </div>



                                    <div class="row">

                                        <div class="col-md-6">
                                            <!-- Example Select Sizing -->
                                            <div class="example-wrap">
                                                <h4 class="example-title">ID Projet</h4>

                                                <div class="example">

                                                    {{ form_widget(edit_form.idprojet,{'attr': {'name':'idprojet', 'type':'text','class': 'form-control', 'id':'idprojet'}}) }}


                                                </div>

                                            </div>
                                            <!-- End Example Select Sizing -->
                                        </div>
                                    </div>







                                    <div class="form-group form-material">
                                        <button type="submit" class="btn btn-primary">Edit</button>
                                    </div>
                                    {{ form_end(edit_form) }}
                                    <div class="form-group form-material">

                                        {{ form_start(delete_form) }}
                                        <button type="submit" class="btn btn-primary" value="Delete">Delete</button>

                                        {{ form_end(delete_form) }}
                                    </div>
                                </div>
                            </div>
                            <!-- End Example Basic Form -->
                        </div>


                    </div>
                </div>
            </div>



        </div>
    </div>
